{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200",
    "DashbaordUri": "http://localhost:5601",
    "loggingIndex": "media-data"
  },
  "ElasticInjectionProperties": {
    "albumIndex": "albums"
  },
  "Properties": {
    "Application": "MediaData",
    "Region": "US",
    "Environment": "Dev"
  }
}

